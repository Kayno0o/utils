{
  "name": "@kaynooo/utils",
  "type": "module",
  "version": "1.5.2",
  "description": "A bunch of small utils functions that I use in my TS based projects",
  "author": "Kaynooo <https://github.com/Kayno0o>",
  "license": "MIT",
  "homepage": "https://Kayno0o.github.io/utils/docs/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kayno0o/utils.git"
  },
  "bugs": {
    "url": "https://github.com/Kayno0o/utils/issues"
  },
  "keywords": [
    "utils",
    "typescript",
    "ts",
    "javascript",
    "js",
    "esm"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    },
    "./decorators": {
      "types": "./dist/decorators/index.d.ts",
      "import": "./dist/decorators/index.js"
    },
    "./format": {
      "types": "./dist/format/index.d.ts",
      "import": "./dist/format/index.js"
    },
    "./tools": {
      "types": "./dist/tools/index.d.ts",
      "import": "./dist/tools/index.js"
    },
    "./i18n/date/*": {
      "types": "./dist/i18n/date/*.d.ts",
      "import": "./dist/i18n/date/*.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check": "tsc --noEmit --strict",
    "build": "rm -rf ./dist && tsup",
    "lint:fix": "bunx --bun eslint --fix",
    "lint": "bunx --bun eslint",
    "doc": "typedoc ./src/**/*.ts --plugin typedoc-github-theme",
    "test": "rm -rf coverage coverage-report && bun test && genhtml -o coverage -c ./assets/gcov.css ./coverage/lcov.info >/dev/null",
    "benchmark": "bun benchmark.ts"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@kaynooo/eslint": "^1.2.0",
    "@types/bun": "^1.2.23",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "mitata": "^1.0.34",
    "tinybench": "^5.0.1",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.13",
    "typedoc-github-theme": "^0.3.1",
    "typescript": "5.9.2"
  },
  "directories": {
    "example": "./examples",
    "test": "./tests"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
