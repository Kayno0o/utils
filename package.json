{
  "name": "@kaynooo/utils",
  "type": "module",
  "version": "1.2.15",
  "description": "A bunch of small utils functions that I use in my TS based projects",
  "author": "Kaynooo <https://github.com/Kayno0o>",
  "license": "MIT",
  "homepage": "https://Kayno0o.github.io/utils/docs/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kayno0o/utils.git"
  },
  "bugs": {
    "url": "https://github.com/Kayno0o/utils/issues"
  },
  "keywords": [
    "utils",
    "typescript",
    "ts",
    "javascript",
    "js",
    "cjs",
    "esm"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "doc": "typedoc ./src/* --plugin typedoc-github-theme",
    "test": "rm -rf coverage coverage-report && bun test && genhtml -o coverage -c ./assets/gcov.css ./coverage/lcov.info >/dev/null",
    "benchmark": "bun benchmark.ts"
  },
  "peerDependencies": {
    "dayjs": "^1.11.13",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@kaynooo/eslint": "^1.1.6",
    "@types/bun": "^1.2.13",
    "eslint": "^9.26.0",
    "lint-staged": "^16.0.0",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.4",
    "typedoc-github-theme": "^0.3.0",
    "typescript": "^5.8.3"
  },
  "directories": {
    "example": "./examples",
    "test": "./tests"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
